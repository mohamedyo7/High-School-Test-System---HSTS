<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="568.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.examsFinalstu">
   <children>
      <TableView fx:id="examsTable" layoutX="35.0" layoutY="73.0" prefHeight="200.0" prefWidth="220.0">
        <columns>
          <TableColumn fx:id="examsTablemini" prefWidth="95.0" text="Exams" />
            <TableColumn fx:id="numberTable" prefWidth="124.0" text="questions Number" />
        </columns>
      </TableView>
      <ListView fx:id="coursesList" layoutX="286.0" layoutY="73.0" prefHeight="200.0" prefWidth="200.0" />
      <Button layoutX="524.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackB" text="Back" />
      <Button fx:id="start_b" layoutX="35.0" layoutY="296.0" mnemonicParsing="false" onAction="#startB" prefHeight="25.0" prefWidth="74.0" text="Start Exam" />
      <Button layoutX="345.0" layoutY="296.0" mnemonicParsing="false" onAction="#showExams" text="Show Exams" />
      <Label layoutX="182.0" layoutY="396.0" />
      <TextField fx:id="eCode" layoutX="42.0" layoutY="371.0" />
      <Text fill="RED" layoutX="42.0" layoutY="357.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter exam code">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="studenid" layoutX="42.0" layoutY="437.0" />
      <Text fill="RED" layoutX="42.0" layoutY="428.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter your id">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Button fx:id="downlodExamid" layoutX="140.0" layoutY="296.0" mnemonicParsing="false" onAction="#download_exam" text="Download Exam" />
      <Label fx:id="submit_exam" layoutX="255.0" layoutY="375.0" text="Please Submit Your Exam" textFill="#f80505" />
      <TextField fx:id="submit_textfield" layoutX="248.0" layoutY="401.0" />
      <Button fx:id="submit_but" layoutX="297.0" layoutY="450.0" mnemonicParsing="false" onAction="#Submit_but" text="Submit" />
      <Text fx:id="errorText" fill="RED" layoutX="35.0" layoutY="289.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
